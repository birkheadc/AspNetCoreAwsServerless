{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Information",
        "Microsoft.AspNetCore.Hosting": "Warning",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext"]
  },
  "AllowedOrigins": ["https://vite-template-staging.birkheadc.me"],
  "Root": {
    "Greeting": "You have reached Colby's ASP.NET Core Aws Serverless Template API.\n\nEnvironment: Staging.\n\nBuild check ICE."
  },
  "Books": {
    "PageSize": "10"
  },
  "AWS": {
    "Region": "ap-southeast-2"
  },
  "DynamoDBContext": {
    "TableNamePrefix": "AspNetCoreServerless_Staging_"
  },
  "Cognito": {
    "ClientId": "ppc3smggblfg66co915k36ud5",
    "Url": "https://aspnetcoreserverless-staging.auth.ap-southeast-2.amazoncognito.com"
  }
}
